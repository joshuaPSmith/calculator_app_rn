{
    "compilerOptions": {
      "importHelpers": true,
      "target": "es5",
      "lib": ["es2015", "esnext.asynciterable"],
      "jsx": "react-native",
      "strict": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "types": [
        "jest"
      ],
      "baseUrl": "src",
      "paths": {
        "common/*": ["./common/*"]
      }
    },
    "exclude": [
      "node_modules/**",
      "typings/**"
  ]
  }